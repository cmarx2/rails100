<h1>Editing Song</h1>

<%= form_for [@billboard, @song] do |f| %>
  <div class="field">
  <%= f.label :rank, "Rank" %>
  <%= f.text_field :rank %>
  </div>

  <div class="field">
  <%= f.label :title, "Title" %>
  <%= f.text_field :title %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>

<%= link_to 'Show', @song %> |
<%= link_to 'Back', songs_path %>
<%= link_to 'Delete Song', [@billboard, @song], method: :delete %>
